package SDE.Enumerador
{
    public final class EDmsTipoDeclaracao
    {
        public static function getCampos():Array{return['com_movimento','sem_movimento']};
        
        public static const com_movimento:String = 'com_movimento';
        public static const sem_movimento:String = 'sem_movimento';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
