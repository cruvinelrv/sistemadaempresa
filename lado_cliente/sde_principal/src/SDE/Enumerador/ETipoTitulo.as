package SDE.Enumerador
{
    public final class ETipoTitulo
    {
        public static function getCampos():Array{return['cheque_a_receber','titulo_a_receber','duplicata_a_receber','titulo_a_pagar']};
        
        public static const cheque_a_receber:String = 'cheque_a_receber';
        public static const titulo_a_receber:String = 'titulo_a_receber';
        public static const duplicata_a_receber:String = 'duplicata_a_receber';
        public static const titulo_a_pagar:String = 'titulo_a_pagar';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
