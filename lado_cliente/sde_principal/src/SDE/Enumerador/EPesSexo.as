package SDE.Enumerador
{
    public final class EPesSexo
    {
        public static function getCampos():Array{return['nao_informado','masculino','feminino']};
        
        public static const nao_informado:String = 'nao_informado';
        public static const masculino:String = 'masculino';
        public static const feminino:String = 'feminino';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
