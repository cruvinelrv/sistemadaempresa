package SDE.Enumerador
{
    public final class EMovImpressao
    {
        public static function getCampos():Array{return['sem_impressao','cupom_fiscal','nfe_produto','orcamento','reserva','nfs_prefeitura','nf_formulario','pedido']};
        
        public static const sem_impressao:String = 'sem_impressao';
        public static const cupom_fiscal:String = 'cupom_fiscal';
        public static const nfe_produto:String = 'nfe_produto';
        public static const orcamento:String = 'orcamento';
        public static const reserva:String = 'reserva';
        public static const nfs_prefeitura:String = 'nfs_prefeitura';
        public static const nf_formulario:String = 'nf_formulario';
        public static const pedido:String = 'pedido';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
