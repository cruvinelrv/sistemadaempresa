package SDE.Enumerador
{
    public final class EVeiculoTipo
    {
        public static function getCampos():Array{return['automovel','caminhao','motocicleta','reboque','semi_reboque','maquina_agricola']};
        
        public static const automovel:String = 'automovel';
        public static const caminhao:String = 'caminhao';
        public static const motocicleta:String = 'motocicleta';
        public static const reboque:String = 'reboque';
        public static const semi_reboque:String = 'semi_reboque';
        public static const maquina_agricola:String = 'maquina_agricola';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
