package SDE.Enumerador
{
    public final class EDmsSituacao
    {
        public static function getCampos():Array{return['normal','retificada']};
        
        public static const normal:String = 'normal';
        public static const retificada:String = 'retificada';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
