package SDE.Enumerador
{
    public final class EOrdemServicoTipo
    {
        public static function getCampos():Array{return['servico','producao','equipamento','veiculo','maq_agricola','maq_pesada']};
        
        public static const servico:String = 'servico';
        public static const producao:String = 'producao';
        public static const equipamento:String = 'equipamento';
        public static const veiculo:String = 'veiculo';
        public static const maq_agricola:String = 'maq_agricola';
        public static const maq_pesada:String = 'maq_pesada';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
