package SDE.Enumerador
{
    public final class ENfeFinalidade
    {
        public static function getCampos():Array{return['normal','ajuste','complementar']};
        
        public static const normal:String = 'normal';
        public static const ajuste:String = 'ajuste';
        public static const complementar:String = 'complementar';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
