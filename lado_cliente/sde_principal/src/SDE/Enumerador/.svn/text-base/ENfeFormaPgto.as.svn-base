package SDE.Enumerador
{
    public final class ENfeFormaPgto
    {
        public static function getCampos():Array{return['vista','prazo','outros']};
        
        public static const vista:String = 'vista';
        public static const prazo:String = 'prazo';
        public static const outros:String = 'outros';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
