package SDE.Enumerador
{
    public final class EItemTipo
    {
        public static function getCampos():Array{return['produto','servico']};
        
        public static const produto:String = 'produto';
        public static const servico:String = 'servico';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
