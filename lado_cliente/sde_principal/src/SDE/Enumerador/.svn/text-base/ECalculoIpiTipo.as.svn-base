package SDE.Enumerador
{
    public final class ECalculoIpiTipo
    {
        public static function getCampos():Array{return['percentual','valor_fixo']};
        
        public static const percentual:String = 'percentual';
        public static const valor_fixo:String = 'valor_fixo';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
