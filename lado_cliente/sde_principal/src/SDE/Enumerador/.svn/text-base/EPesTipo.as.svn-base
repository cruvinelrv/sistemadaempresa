package SDE.Enumerador
{
    public final class EPesTipo
    {
        public static function getCampos():Array{return['nao_informado','Fisica','Juridica']};
        
        public static const nao_informado:String = 'nao_informado';
        public static const Fisica:String = 'Fisica';
        public static const Juridica:String = 'Juridica';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
