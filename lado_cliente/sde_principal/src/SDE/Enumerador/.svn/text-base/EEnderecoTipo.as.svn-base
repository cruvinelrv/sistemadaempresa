package SDE.Enumerador
{
    public final class EEnderecoTipo
    {
        public static function getCampos():Array{return['residencial_comercial','rural']};
        
        public static const residencial_comercial:String = 'residencial_comercial';
        public static const rural:String = 'rural';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
