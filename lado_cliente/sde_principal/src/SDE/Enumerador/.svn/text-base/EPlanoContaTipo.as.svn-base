package SDE.Enumerador
{
    public final class EPlanoContaTipo
    {
        public static function getCampos():Array{return['Im','RF','RV','DF','DV']};
        
        public static const Im:String = 'Im';
        public static const RF:String = 'RF';
        public static const RV:String = 'RV';
        public static const DF:String = 'DF';
        public static const DV:String = 'DV';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
