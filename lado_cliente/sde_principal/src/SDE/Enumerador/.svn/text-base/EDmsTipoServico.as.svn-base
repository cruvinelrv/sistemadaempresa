package SDE.Enumerador
{
    public final class EDmsTipoServico
    {
        public static function getCampos():Array{return['prestado','tomado']};
        
        public static const prestado:String = 'prestado';
        public static const tomado:String = 'tomado';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
