package SDE.Enumerador
{
    public final class EContatoTipo
    {
        public static function getCampos():Array{return['fone_fixo','celular','email','msn','skype','fax','ddg','homepage']};
        
        public static const fone_fixo:String = 'fone_fixo';
        public static const celular:String = 'celular';
        public static const email:String = 'email';
        public static const msn:String = 'msn';
        public static const skype:String = 'skype';
        public static const fax:String = 'fax';
        public static const ddg:String = 'ddg';
        public static const homepage:String = 'homepage';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
