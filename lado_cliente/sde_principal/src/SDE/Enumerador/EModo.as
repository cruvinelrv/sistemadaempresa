package SDE.Enumerador
{
    public final class EModo
    {
        public static function getCampos():Array{return['desenvolvimento','suporte','teste','producao']};
        
        public static const desenvolvimento:String = 'desenvolvimento';
        public static const suporte:String = 'suporte';
        public static const teste:String = 'teste';
        public static const producao:String = 'producao';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
