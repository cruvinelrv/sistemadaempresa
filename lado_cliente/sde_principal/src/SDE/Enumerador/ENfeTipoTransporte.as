package SDE.Enumerador
{
    public final class ENfeTipoTransporte
    {
        public static function getCampos():Array{return['emitente','destinatario']};
        
        public static const emitente:String = 'emitente';
        public static const destinatario:String = 'destinatario';
        
        public static function valida(value:String):Boolean
        {
            return (getCampos().indexOf(value)>-1);
        }
    }
}
