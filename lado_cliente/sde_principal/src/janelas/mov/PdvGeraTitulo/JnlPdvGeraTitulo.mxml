<?xml version="1.0" encoding="utf-8"?>
<mx:VBox xmlns:mx="http://www.adobe.com/2006/mxml" xmlns:CP="Componentes.CP.*" xmlns:comp="Componentes.comp.*" xmlns:Combo="SDE.Componentes.Combo.*"
	width="100%" height="100%"
	verticalGap="0"
	color="#333333"
	styleName="padding3"
	keyDown="botoesOcultos(event)"
	preinitialize="init()"
	creationComplete="create();
	vendedor_selecionado = cache.getCliente(Sessao.unica.idClienteFuncionarioLogado);
	cliente_selecionado = cache.getCliente(1).clone();
	cpCliente.setFocus();"
	xmlns:PopUpPesquisa="Componentes.PopUpPesquisa.*" xmlns:Combo1="Componentes.Combo.*" xmlns:CP1="Componentes.CaixaPesquisa.CP.*" xmlns:ns="Componentes.CP.CP_Rend.*"
	xmlns:master="com.flexpernambuco.controls.*" xmlns:SDE="Componentes.SDE.*" xmlns:Paginacao="Componentes.Paginacao.*">
	<mx:Fade id="quickFade" duration="200"/>
	
	<mx:DateValidator id="validadorData"
		source="{popupDataPedido_data}" property="text"
		inputFormat="DD/MM/YYYY"
		allowedFormatChars="/"/>
	
	<mx:Script>
		<![CDATA[
			import Core.Sessao;
			import mx.formatters.Formatter;
			import SDE.Enumerador.EMovImpressao;
			import mx.events.CloseEvent;
			import com.hillelcoren.components.CloseableTitleWindow;
			import SDE.Entidade.SdeConfig;
			import SDE.Entidade.Cliente;
			import SDE.Constantes.Variaveis_SdeConfig;
			import mx.core.Application;
			import mx.managers.PopUpManager;
			import Core.App;
			import SDE.Entidade.ClienteEndereco;
			import SDE.Enumerador.EItemTipoIdent;
			import SDE.Enumerador.EItemTipo;
			import Core.Utils.Formatadores;
			import Core.Utils.Funcoes;
			import mx.controls.Alert;
			import SDE.Entidade.MovItem;
			import Core.Alerta.AlertaSistema;
			import img.Imagens;
		]]>
	</mx:Script>
	
	<mx:Script source="script1.as"/>
	<mx:Script source="JnlPdvGeraTitulo_script_dadoscliente.as"/>
	<mx:Script source="JnlPdvGeraTitulo_script_formaspagamento.as"/>
	<mx:Script source="JnlPdvGeraTitulo_script_finalizacao.as"/>
	<mx:Script source="JnlPdvGeraTitulo_script_importar_mov.as"/>
	<mx:Script source="JnlPdvGeraTitulo_script_importar_os.as"/>
	<mx:Script source="JnlPdvGeraTitulo_script_notaFiscal.as"/>
	<mx:Script source="JnlPdvGeraTitulo_script_calculos.as"/>
	
	<mx:CurrencyFormatter id="cfReal"
		precision="2"
		rounding="none"
		decimalSeparatorTo="."
		thousandsSeparatorTo=","
		useThousandsSeparator="true"
		currencySymbol="R$"
		alignSymbol="left"
		useNegativeSign="true"
		/>
		
	<mx:CurrencyFormatter id="cfPorcentagem"
		precision="2"
		rounding="none"
		decimalSeparatorTo="."
		thousandsSeparatorTo=","
		useThousandsSeparator="true"
		currencySymbol="%"
		alignSymbol="right"
		useNegativeSign="true"
		/>
	
	<PopUpPesquisa:PopPesquisa_Vendas
		id="popup_importar_venda"
		retorna="{ usuario_retorna_popup_importa_mov(event) }"
		close="{ usuario_fecha_popup_importa_mov() }"
		/>
		
	<PopUpPesquisa:PopPesquisa_OrdemServico
		id="popup_importar_ordemServico"
		retorna="{ usuario_retorna_popup_importa_os(event) }"
		close="{ usuario_fecha_popup_importa_os() }"
		/>
	
	<mx:HBox width="100%"
		backgroundColor="#3B5998" color="white" fontSize="16"
		fontWeight="bold" styleName="padding0"
		horizontalAlign="right">
		
		<mx:HBox width="100%"
			backgroundColor="#3B5998" color="white" fontSize="16"
			fontWeight="bold" styleName="padding0"
			horizontalAlign="left">
			<mx:Button label="LANÇAR VALOR FINAL"
				click="{ usuario_abre_popupDescFinal()}" visible="{descVlrFinal}"/>
		</mx:HBox>
		
		<mx:Grid>
			<mx:GridRow horizontalAlign="right">			
				<mx:GridItem>
					<mx:Label fontSize="14" text="Qtde: "/>
					<mx:Label fontSize="17" text="{indicador_itenscarrinho_qtd}" />
				</mx:GridItem>
				<mx:GridItem>
					<mx:Label fontSize="14" text="Bruto: " />
					<mx:Label fontSize="17" text="{Formatadores.unica.formataValor(indicador_itenscarrinho_valor_bruto,true)}" />
				</mx:GridItem>
				<mx:GridItem visible="{indicador_itenscarrinho_valor_desconto != 0}">
					<mx:Label id="lbDesconto" fontSize="14" text="Desconto: " />
					<mx:Label fontSize="17" text="{Formatadores.unica.formataValor(indicador_itenscarrinho_valor_desconto,true)}" />
				</mx:GridItem>
				<mx:GridItem visible="{indicador_itenscarrinho_valor_juro != 0}">
					<mx:Label fontSize="14" text="Juro: " />
					<mx:Label fontSize="17" text="{Formatadores.unica.formataValor(indicador_itenscarrinho_valor_juro,true)}" />
				</mx:GridItem>
				<mx:GridItem>
					<mx:Label fontSize="14" text="Liq: "/>
					<mx:Label fontSize="17" text="{Formatadores.unica.formataValor(indicador_venda_valor_final,true)}" />
				</mx:GridItem>
				<mx:GridItem id="gridItemFinal" visible="false">
					<mx:Label id="lbFinal" fontSize="14" text="Final: " />
					<mx:Label fontSize="17" text="{Formatadores.unica.formataValor(indicador_venda_valor_final,true)}" />
				</mx:GridItem>
			</mx:GridRow>
		</mx:Grid>
	</mx:HBox>
	
	<mx:HBox verticalAlign="middle" visible="{vs.selectedIndex==0}">
		<mx:Label fontSize="14" fontWeight="bold" text="Produto Selecionado:" />
		<mx:Label minWidth="200" fontSize="20" fontWeight="bold" text="{item_para_carrinho.nome}"/>
	</mx:HBox>
	<mx:HBox horizontalAlign="center" themeColor="#3B5998" width="100%">
		<mx:HRule width="90%"/>
	</mx:HBox>

	<mx:ViewStack id="vs" creationPolicy="all"
		width="100%" height="100%">
		
		<mx:HBox id="etapa1"
			width="100%" height="100%"
			showEffect="{quickFade}"
			hideEffect="{quickFade}">					
			
			<mx:VBox id="caixa"
				width="100%" height="100%"
				fontSize="14" fontWeight="bold">
				
				<mx:Label text="Cliente:" />
				<CP:PesqCliente id="cpCliente" width="100%" change="{cliente_selecionado = cpCliente.selectedItem as Cliente; change_cpCliente();}"
					keyDown="if(proximoFoco(event)){cpVendedor.setFocus();}" enabled="{!isImportadoOS}"/>
				
				<mx:Label text="Vendedor:" />
				<CP:PesqFuncionario id="cpVendedor" width="100%" change="{ change_cpVendedor() }"
					keyDown="if(proximoFoco(event)){cpItem.txtPesquisaBox.setFocus();}" enabled="{!isImportadoOS}"/>
				
				<mx:TabNavigator id="tabNav" width="{caixa.width}" height="330"
					creationPolicy="all">
					
					<mx:VBox 
						icon="{Imagens.unica.icn_32_estoque}"
						label="Busca Produtos"
						width="100%" height="100%"
						enabled="{!isImportadoOS}">
		
					<mx:HBox width="100%" horizontalAlign="right">
						<mx:Label text="Produto:" width="100%"/>
						<mx:Button id="btnEscolheEstoque" visible="false" label="Estoque" click="{ abrepopup_estoques() }" width="90"/>
					</mx:HBox>			
					
					<CP:CPesqItem width="100%" id="cpItem" change="{ usuario_escolheu_item_para_carrinho() }"/>
					
					<mx:Label text="Estoque:" />
					
					<SDE:CaixaPesquisa id="cpEstoque" icone="{null}" width="100%" clickPesquisar="usuario_escolheu_estoque_para_carrinho()"/>
					
						<mx:Grid verticalAlign="middle">
							<mx:GridRow width="100%" height="100%" enabled="{item_para_carrinho!=null}">
								<mx:GridItem>
									<mx:Label text="Quant (max: {(estoque_para_carrinho!=null)?estoque_para_carrinho.qtd:0})" />
								</mx:GridItem>
								<mx:GridItem>
									<mx:NumericStepper id="nsQtd" width="100%" maximum="9999999" stepSize="0.0001"
											keyDown="{
												if(event.keyCode==Keyboard.ENTER)
													usuario_define_quantidade();
											}"
										/>
								</mx:GridItem>
								<mx:GridItem>
									<mx:Label text="Impostos:" />
								</mx:GridItem>
								<mx:GridItem>
									<comp:Botao width="{nsQtd.width}" height="{nsQtd.height}" icon="{Imagens.unica.icn_16_dollar}" label="Impostos" click="usuario_abre_popup_impostos()"/>
								</mx:GridItem>
							</mx:GridRow>
							<mx:GridRow width="100%" height="100%" enabled="{preco_para_carrinho!=null}">
								<mx:GridItem>
									<mx:Label text="Valor Unitário:" />
								</mx:GridItem>
								<mx:GridItem >
									<mx:NumericStepper id="nsVlrUnit" width="100%" maximum="9999999" stepSize="0.01"
										keyDown="{
											if (event.keyCode==Keyboard.ENTER)
												usuario_lanca_item_para_carrinho();
										}"
										valueCommit="{ nsVlrTotal.value = nsVlrUnit.value * nsQtd.value; }"
										/>
								</mx:GridItem>
								<mx:GridItem>
									<mx:Label text="Valor Total:" />
								</mx:GridItem>
								<mx:GridItem>
									<mx:NumericStepper id="nsVlrTotal" width="100%" maximum="9999999" stepSize="0.01"
										valueCommit="{}"
										keyDown="if(proximoFoco(event)){cpItem.txtPesquisaBox.setFocus();}"
										/>
								</mx:GridItem>
							</mx:GridRow>
						</mx:Grid>
					<mx:HBox width="100%">
						<mx:Label text="CFOP:" />
						<CP:PesqCFOP id="cpCFOP" width="100%" showBrowseButton="false" fontSize="10" change="cpCFOP_Change()"/>
					</mx:HBox>		
					<comp:Botao2 label="LANÇAR"
						click="{ usuario_lanca_item_para_carrinho() }"
						enabled="{preco_para_carrinho!=null}"
						keyDown="if(proximoFoco(event)){cpItem.txtPesquisaBox.setFocus();}"/>				
					</mx:VBox>
					<mx:VBox
						icon="{Imagens.unica.icn_32_carrinho}"
						label="Importar"
						horizontalAlign="center"
						textAlign="center" width="100%" height="100%">
									<SDE:Botao
										height="85" width="95%" cornerRadius="0"
										icon="{Imagens.unica.icn_64_orcamento}" label="Pesquisar Movimentações" fontSize="22"
										click=
										"{
											usuario_abre_popup_importa_mov();
											popup_importar_venda.currentState = null;
										}"
									/>
									<SDE:Botao
										visible="{mostraImportarOS}"
										height="85" width="95%" cornerRadius="0"
										icon="{Imagens.unica.icn_64_ordem_servico}" label="Pesquisar Ordem de Serviço" fontSize="22"
										click=
										"{
											usuario_abre_popup_importa_os();
											popup_importar_ordemServico.currentState = null;
										}"
									/>
									<SDE:Botao
										visible="{isImportadoOS}"
										height="85" width="95%" cornerRadius="0"
										icon="{Imagens.unica.icn_64_fecha}" label="Cancelar Importação de OS" fontSize="22"
										click="sistema_resetar_tela()"
									/>
								</mx:VBox>
							
								<mx:VBox label="Opções" icon="{Imagens.unica.icn_32_config}"
									horizontalAlign="center" verticalAlign="middle"
									width="100%" styleName="padding5"
									enabled="{arraycItensCarrinho.length > 0}">
									
									<mx:HBox width="80%" verticalAlign="middle" enabled="{!isDescontoLancado}">
										<mx:Label text="Valor Bruto:" fontWeight="bold"/>
										<mx:Label id="desc_valorbruto" fontWeight="bold"
											text="{Formatadores.unica.formataValor(indicador_itenscarrinho_valor_bruto, true)}"/>
									</mx:HBox>
									
									<mx:HBox width="80%" verticalAlign="middle" enabled="{!isDescontoLancado}">
										<mx:Label text="Desconto Unitário:" fontWeight="bold"/>
										<mx:Label id="desc_unit" fontWeight="bold"
											text="{Formatadores.unica.formataValor(indicador_itenscarrinho_valor_desconto_unit, true)}"/>
									</mx:HBox>
									
									<mx:HBox width="80%" verticalAlign="middle" enabled="{!isDescontoLancado}">
										<mx:Label text="Subtotal:" fontWeight="bold"/>
										<mx:Label id="subtotal" fontWeight="bold"
											text="{Formatadores.unica.formataValor(indicador_itenscarrinho_valor_bruto - indicador_itenscarrinho_valor_desconto_unit, true)}"/>
									</mx:HBox>
									
									<mx:HRule width="95%" height="2"/>
									
									<mx:HBox width="80%" verticalAlign="middle" enabled="{!isDescontoLancado}">
										<mx:RadioButton id="rbEmReais" groupName="rbGroupDesc"/>
										<mx:Label text="Desconto em Reais:" fontWeight="bold" width="200"/>
										<mx:NumericStepper id="desconto_emValor" width="150" fontWeight="bold" maximum="9999999" stepSize="0.01" enabled="{rbEmReais.selected}"
											keyDown="if (event.keyCode == Keyboard.ENTER){sistema_calcular_valor_abaDesconto();}"/>
									</mx:HBox>
									
									<mx:HBox width="80%" verticalAlign="middle" enabled="{!isDescontoLancado}">
										<mx:RadioButton id="rbEmPercent" groupName="rbGroupDesc"/>
										<mx:Label text="Desconto em Porcentagem:" fontWeight="bold" width="200"/>
										<mx:NumericStepper id="desconto_emPct" width="150" fontWeight="bold" maximum="100" stepSize="0.01" enabled="{rbEmPercent.selected}"
											keyDown="if (event.keyCode == Keyboard.ENTER){sistema_calcular_valor_abaDesconto();}"/>
									</mx:HBox>
									
									<mx:HBox width="80%" verticalAlign="middle" enabled="{!isDescontoLancado}">
										<mx:RadioButton id="rbVlrFinal" groupName="rbGroupDesc"/>
										<mx:Label text="Valor Final em Reais:" fontWeight="bold" width="200"/>
										<mx:NumericStepper id="desconto_final" width="150" fontWeight="bold" maximum="9999999" stepSize="0.01" enabled="{rbVlrFinal.selected}"
											keyDown="if (event.keyCode == Keyboard.ENTER){sistema_calcular_valor_abaDesconto();}"/>
									</mx:HBox>
									
									<mx:HBox verticalAlign="middle" width="80%">
										<SDE:Botao id="btnSalvaDesc" label="Salvar Desconto" labelPlacement="right" icon="{Imagens.unica.icn_32_salva}"
											click="sistema_calcular_valor_carrinho(); isDescontoLancado = true;"/>
										<SDE:Botao id="btnRemoveDesc" label="Remover Desconto" labelPlacement="right" icon="{Imagens.unica.icn_32_deleta}" enabled="{isDescontoLancado}"
											click="remove_desconto(); isDescontoLancado = false;"/>
									</mx:HBox>
									
								</mx:VBox>
						</mx:TabNavigator>
					</mx:VBox>
		
			<mx:VBox
				width="100%" height="100%">
				
				<mx:Label fontSize="14" fontWeight="bold" text="Produtos Vendidos" />
				<mx:DataGrid id="dtGridMI" dataProvider="{arraycItensCarrinho}" width="100%" height="100%"
					themeColor="#3B5998">
					<mx:columns>
						<mx:DataGridColumn width="0.8" dataField="qtd" headerText="Qtde"/>
						<mx:DataGridColumn width="4" dataField="item_nome" headerText="Produto"/>
						<comp:MyDataGridColumn width="1.5" dataField="vlrUnitVendaFinal" headerText="R$ Unit" tipo="Dinheiro"/>
						<comp:MyDataGridColumn width="1.5" dataField="vlrUnitVendaFinalQtd" headerText="R$ Total" tipo="Dinheiro"/>
						<mx:DataGridColumn width="1" dataField="icmsAliq" headerText="icms"/>
						<mx:DataGridColumn width="0.8" dataField="cfop" headerText="CFOP"/>
						<mx:DataGridColumn width="1" headerText=" ">
							<mx:itemRenderer>
								<mx:Component>
									<mx:Button
										width="22" themeColor="#840000"
										label="" icon="{Imagens.unica.icn_16_cancela}"
										click="dispatchEvent(new Event('deleteRow', true))">
										<mx:Script>
											<![CDATA[
												import img.Imagens;
											]]>
										</mx:Script>
									</mx:Button>
								</mx:Component>
							</mx:itemRenderer>
						</mx:DataGridColumn>
					</mx:columns>
				</mx:DataGrid>
			</mx:VBox>
			<mx:VBox horizontalAlign="left" verticalAlign="middle" height="100%">
				<mx:Button label="Forma de Pagto" icon="{Imagens.unica.icn_64_seta_dir}" labelPlacement="bottom"
					width="100" fontSize="9" click="vs.selectedChild = etapa2; verificaEstoque();" />
			</mx:VBox>
		</mx:HBox>
			
		<mx:HBox id="etapa2"
			width="100%" height="100%"
			fontSize="20" show="{ nsVlrFatiaTitulo.setFocus();}"
			showEffect="{quickFade}"
			hideEffect="{quickFade}">
			<mx:VBox height="100%" verticalAlign="middle">
				<mx:Button label="Venda" icon="{Imagens.unica.icn_64_seta_esq}" labelPlacement="bottom"
					width="100" fontSize="9" click="{vs.selectedChild = etapa1, usuario_reseta_titulos() }"/>
			</mx:VBox>
			<mx:VBox width="100%" height="100%">
				<mx:HBox width="100%">
					<mx:VBox>
						<mx:HBox>
							<mx:Label text="Total" width="100" />
							<mx:Label text="R$" />
							<mx:Label text="{Formatadores.unica.formataValor(indicador_itenscarrinho_valor_liquido,false)}" />
							<mx:Label text="Juro de Parcelamento" />
							<mx:Label text="R$" />
							<mx:Label text="{Formatadores.unica.formataValor(indicador_venda_valor_juro_parcelamento,false)}" />
						</mx:HBox>	
						<mx:HBox>
							<mx:Label text="Parte" width="100" />
							<mx:Label text="R$" />
							<mx:NumericStepper width="100" id="nsVlrFatiaTitulo"
								keyDown="{
									if (event.keyCode==Keyboard.ENTER)
									{
										if(nsVlrFatiaTitulo.value>0)
										{								
											cmbTiposPgto.setFocus();
										}
									}
									if (event.keyCode==Keyboard.ENTER)
									{
										if(nsVlrFatiaTitulo.value==0)
										{
											btFinalizacao.setFocus();
										}
									}
								}"
								maximum="{indicador_itenscarrinho_valor_liquido}"
								minimum="0.00" stepSize="0.01"
								enabled="{ indicador_venda_valor_pagamento_indefinido > 0 }"/>
							<Combo:CmbFinan_TipoPagamento id="cmbTiposPgto" minWidth="250"
								enabled="{nsVlrFatiaTitulo.value > 0 }"
								keyDown="{ if (event.keyCode==Keyboard.ENTER) btnLancaTituloParaEdicao.setFocus(); }"/>
							<comp:Botao2 width="120" id="btnLancaTituloParaEdicao" cor="Azul" label="Lançar"
								click="{ usuario_lanca_fatia_pagamento() }"
								enabled="{ nsVlrFatiaTitulo.value > 0 }"
								keyDown="{
									if(nsVlrFatiaTitulo.value>0)
										if (event.keyCode==Keyboard.ENTER)
											usuario_lanca_fatia_pagamento();}"/>
							<comp:Botao2 width="120" cor="Azul" label="Recomeçar"
								click="{ usuario_reseta_titulos() }"
								keyDown="{
									if (event.keyCode==Keyboard.ENTER)
										usuario_reseta_titulos()}"/>
						</mx:HBox>
					</mx:VBox>
				</mx:HBox>
				<mx:DataGrid id="dtGridTitulosGerados"
					width="100%" height="100%"
					themeColor="#3B5998"
					rowHeight="30"
					dataProvider="{ arraycLancamentosCaixa }">
					<mx:columns>
						<mx:DataGridColumn width="1.5" dataField="tipoPagamento_nome" headerText="Tipo"/>
						<mx:DataGridColumn width="1.5" dataField="dtPagamento" headerText="Vencimento"/>
						<comp:MyDataGridColumn width="1.5" dataField="valorOriginal" headerText="Original" tipo="Dinheiro"/>
						<comp:MyDataGridColumn width="1.5" dataField="valorCobrado" headerText="Cobrado" tipo="Dinheiro"/>
						<mx:DataGridColumn width="3" dataField="observacoes" headerText="Observacoes"/>
						<mx:DataGridColumn width="1">
							<mx:itemRenderer>
								<mx:Component>
									<mx:Button
										width="22" color="#840000"
										label="remover" fontSize="12"
										click="dispatchEvent(new Event('deleteRow', true))" />
								</mx:Component>
							</mx:itemRenderer>
						</mx:DataGridColumn>
					</mx:columns>
				</mx:DataGrid>
			</mx:VBox>
			<mx:VBox height="100%" verticalAlign="middle">
				<mx:Button id="btFinalizacao" label="Finalização" icon="{Imagens.unica.icn_64_seta_dir}" labelPlacement="bottom"
					width="100" fontSize="9"
					keyDown=
					"{
						if(event.keyCode==Keyboard.ENTER)
						{
							mostraValorFinal();
							if(proximoFoco(event))
							{
								cmbClienteContato.setFocus();
							}
						}
					}"
					click="{mostraValorFinal()}"/>
			</mx:VBox>
		</mx:HBox>
			
		<mx:Canvas id="etapa3"
			show="etapa3.setFocus()"
			click="etapa3.setFocus()"
			width="100%" height="100%"
			showEffect="{quickFade}"
			hideEffect="{quickFade}">
			
			<mx:HBox width="100%" height="100%">
				<mx:HBox width="100%" height="100%">
					<mx:VBox height="100%" verticalAlign="middle">
						<mx:Button label="Forma de Pagto" icon="{Imagens.unica.icn_64_seta_esq}" labelPlacement="bottom"
							width="100" fontSize="9" click="{ocultaValorFinal()}" />
					</mx:VBox>			
				<mx:VBox width="100%" height="100%"
					fontWeight="bold" fontSize="16">
					<mx:HBox verticalAlign="middle">
						<mx:Image source="{Imagens.unica.icn_64_telefone}"/>
						<mx:Image source="{Imagens.unica.icn_64_endereco}"/>
						<mx:Label fontSize="24" text="Contato e Endereços:"/>
					</mx:HBox>
					
					<mx:Label text="Contato:" />
					<Combo1:CmbClienteContato id="cmbClienteContato" width="100%" change="{cmbClienteContato_change()}" />
					
					<mx:Label text="Endereço de Faturamento:" />
