body
{
	line-height: 1.0em;
	margin: 0px;
	padding: 0px;
	/*border-right: black dashed 3px;*/
	width: 28cm;
	height: 21cm;
}
body.carregando
{
	background: url('../img/ajax-loader1.gif') no-repeat fixed center center;
}

.tabela
{
	font-family: "Lucida Sans Unicode", "Lucida Grande", Sans-Serif;
	font-size: 12px;
	background: #fff;
	margin: 5px;
	width: 100%;
	border-collapse: collapse;
	text-align: left;
}
.tabela th
{
	font-size: 14px;
	font-weight: normal;
	color: #000000;
	padding: 10px 8px;
	border-bottom: 2px solid #808080;
}
.tabela td
{
	border-bottom: 1px solid #808080;
	color: #000000;
	padding: 6px 8px;
}
.tabela tbody tr:hover td
{
	color: #808080;
}
.tabela td p 
{
	
}


.tabela_mov
{
    font-family: Roman;
    font-size: 12px;
    background: #fff;
    margin: 5px;
    width: 100%;
    border-collapse: collapse;
    text-align: left;
}
.tabela_mov th
{
    font-size: 12px;
    font-weight: normal;
    color: #000000;
    padding: 2px 1px;
}
.tabela_mov td
{
    border-top: 1px dashed #808080;
    color: #000000;
    padding: 3px 8px;
}
.tabela_mov tbody tr:hover td
{
    color: #808080;
}

.tabela_head
{
	border-top: 1px solid #808080;
	font-family: Roman;
    font-size: 12px;
    background: #fff;
    margin: 5px;
    width: 100%;
    border-bottom: 1px solid #808080;
}
.tabela_head tr
{
	width: 100%;
	color: #000000;
	text-align: left;
}

.tabela_ass
{
	font-family: Roman;
    font-size: 14px;
    background: #fff;
    margin: 5px;
    width: 100%;
}
.tabela_ass tr
{
	width: 100%;
	color: #000000;
	text-align: center;
}
.tabela_ass  td
{
    text-align: left;
    font-size: 12px;
	color: #000000;
	border-top: 1px solid #808080;
}

.tabela_iee
{
	font-family: "Lucida Sans Unicode", "Lucida Grande", Sans-Serif;
	font-size: 12px;
	background: #fff;
	width: 100%;
